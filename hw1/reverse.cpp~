/*

Name: Tanuja Mohan

Description: Read from a file and reverse the content of the file, printing each word on a different line. This is problem 5 of CS104 Assingment 1.

*/


#include <iostream>
#include <fstream>

using namespace std;

int main(int argc, char* argv[]) {
   if (argc != 2)
   {
      cerr << "Incorrect number of arguments" << endl;
      return -1;
   }
   
   ifstream inFile;
   inFile.open(argv[1]);
   
   if (inFile.fail())
   {
      cerr << "Unable to open file" << endl;
      return -1;
   } 
   
   int n = 0;
   inFile >> n;
   if (inFile.fail())
   {
     cerr << "Unable to find starting integer in file" << endl; 
     return -1;
   }
   
   string* fileArray = new string[n];
   
   for (int i = 0; i < n; i++)
   {
      inFile >> fileArray[i];
   }
   
   for (int j = n-1; j >= 0; j--)
   {
      cout << fileArray[j] << endl;
   }
   
   delete[] fileArray;
}
